<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:BlockPuzzle.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:BlockPuzzle.Views"
        mc:Ignorable="d" d:DesignWidth="500" d:DesignHeight="900"
		Width="500"
		Height="900"
		WindowStartupLocation="CenterScreen"
        x:Class="BlockPuzzle.Views.MainWindow"
		x:DataType="vm:MainWindowViewModel"
        Icon="/Assets/avalonia-logo.ico"
        Title="BlockPuzzle">
        <views:MainView DataContext="{Binding MainView}"/>
</Window>
